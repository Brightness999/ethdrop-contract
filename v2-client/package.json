{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.1",
  "author": "Jim Lynch <jim.lynch@evaluates2.com>",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "tslint": "tslint \"src/**/*.ts[x]\" --fix",
    "lint": "npm run format && npm run tslint",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --watch",
    "test-once": "jest  --coverage",
    "e2e": "node_modules/.bin/cypress open",
    "e2e-headless": "node_modules/.bin/cypress run",
    "deploy-ghold": "git push origin :gh-pages && git subtree push --prefix public/ origin gh-pages",
    "deploy-gh": "gh-pages -d public",
    "deploy-s3": "gatsby-plugin-s3 deploy --yes"
  },
  "dependencies": {
    "@types/jest": "^27.0.1",
    "@types/react-helmet": "^6.1.2",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-mock-store": "^1.0.1",
    "gatsby": "^3.12.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-manifest": "^3.12.0",
    "gatsby-plugin-offline": "^4.12.0",
    "gatsby-plugin-react-helmet": "^4.12.0",
    "gatsby-plugin-react-redux": "^1.1.0-0",
    "gatsby-plugin-sharp": "^3.12.0",
    "gatsby-plugin-styled-components": "^4.12.0",
    "gatsby-plugin-typescript": "^3.12.0",
    "gatsby-source-filesystem": "^3.12.0",
    "gatsby-transformer-sharp": "^3.12.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-localstorage-simple": "^2.1.6",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "babel-jest": "^27.0.6",
    "babel-plugin-styled-components": "^1.13.2",
    "babel-preset-gatsby": "^1.12.0",
    "cypress": "^8.3.0",
    "cypress-cucumber-preprocessor": "^4.2.0",
    "gh-pages": "^3.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "serverless-offline": "^8.0.0",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "wait-on": "^6.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "env": {
    "cypress/globals": true
  }
}
