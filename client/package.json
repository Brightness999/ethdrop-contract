    "gatsby": "^3.12.0",
    "gatsby-plugin-google-analytics": "^3.12.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-react-helmet": "^4.12.0",
    "gatsby-plugin-react-redux": "^1.1.0",
    "gatsby-plugin-sass": "^4.12.0",
    "gatsby-plugin-typescript": "^3.12.0",
    "gatsby-source-filesystem": "^3.12.0",
    "gatsby-transformer-json": "^3.12.0",
    "gatsby-transformer-remark": "^4.9.0",
    "postcss": "^8.3.6",
    "react": "^17.0.2",
